class Solution(object):
    def strStr(self, haystack, needle):
        """
        :type haystack: str
        :type needle: str
        :rtype: int
        """
        a=""
        if needle=="":
            return 0
        for i in range(len(haystack)-len(needle)+1):
            for j in range(i,i+len(needle)):
                a=a+haystack[j]
            if needle==a:
                return i
            a=""
            
        return -1    
